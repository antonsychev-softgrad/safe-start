{
    "description": "User login response",
    "type": "object",
    "properties": {
        "meta": {
            "description": "Response meta",
            "type": "object",
            "required": true,
            "properties": {
                "request_id": {
                    "description": "Unique ID of request",
                    "type": "string"
                },
                "status": {
                    "description": " HTTP status code",
                    "type": "integer",
                    "required": true
                }
            }
        },
        "data": {
            "description": "Response data",
            "type": "object",
            "required": true,
            "properties": {
                "auth_token": {
                    "description": "Session ID",
                    "type": "string",
                    "required": true
                },
                "auth_code": {
                    "description": "Authentication Status Code",
                    "type": "integer",
                    "required": true
                },
                "auth_message": {
                    "description": "Authentication status message",
                    "type": "string",
                    "required": true
                }
            }
        }
    }
}