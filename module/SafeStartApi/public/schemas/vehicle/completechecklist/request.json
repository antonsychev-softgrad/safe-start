{
    "description": "Complete Checklist request",
    "type": "object",
    "properties": {
        "meta": {
            "description": "Request meta",
            "type": "object",
            "properties": {
                "requestId": {
                    "description": "Unique ID of request",
                    "type": "string",
                    "required": false
                }
            }
        },
        "data": {
            "description": "Request data",
            "type": "object",
            "required": true,
            "properties": {
                "date": {
                    "description": "Date of filling",
                    "type": "integer",
                    "required": true
                },
                "fields": {
                    "description": "Fields with data",
                    "type": "array",
                    "required": true,
                    "items": {
                        "description": "Field",
                        "type": "object",
                        "required": false,
                        "properties": {
                            "id": {
                                "description": "Field id",
                                "type": "integer",
                                "required": true
                            },
                            "value": {
                                "description": "Field value",
                                "type": "string",
                                "required": false
                            },
                            "alert": {
                                "description": "Field alert",
                                "type": "object",
                                "required": false,
                                "properties": {
                                    "comment": {
                                        "description": "Comment",
                                        "type": "string",
                                        "required": false
                                    },
                                    "photo": {
                                        "description": "Photo's URL",
                                        "type": "string",
                                        "required": false
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}